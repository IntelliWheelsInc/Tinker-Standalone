<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta charset="utf-8" />
		<title>Wheelchairs</title>
		<link rel='stylesheet'  href="{{INVOICE_CSS_URI}}/invoice.css" rel="stylesheet"/>
		<link href="{{INVOICE_CSS_URI}}/idGeneratedStyles.css" rel="stylesheet" type="text/css" />
	</head>
	<body id="Invoice_V3" lang="en-GB">
		{{#each this}}
			<div class="_idGenObjectLayout-1">
				<div id="_idContainer050">
					<div id="_idContainer035">
						<div id="_idContainer031" class="Basic-Graphics-Frame _idGenObjectStyleOverride-1">
						</div>
						<div id="_idContainer032" class="Basic-Text-Frame">
						</div>
						<div id="_idContainer033" class="Basic-Text-Frame">
							<p class="Header">Powered By </p>
						</div>
						<div id="_idContainer034" class="_idGenObjectStyleOverride-2">
							<img class="_idGenObjectAttribute-1" src="data:image/png;base64," alt="" />
						</div>
					</div>
					<div id="_idContainer036" class="Basic-Text-Frame">
						<p class="Section-Title">{{this.wheelchair.title}}</p>
					</div>
					<div id="_idContainer037" class="Basic-Text-Frame">
						<p class="Subtitle" lang="en-US">Manufacturer: </p>
						<p class="Subtitle" lang="en-US">Price:</p>
						<p class="Subtitle" lang="en-US">Model:</p>
						<p class="Subtitle" lang="en-US">Design Date:</p>
						<p class="Subtitle" lang="en-US">Quantity:</p>
              <p class="Subtitle" lang="en-US">Grant Amount:</p>

          </div>
					<div id="_idContainer038" class="Basic-Graphics-Frame _idGenObjectStyleOverride-1">
					</div>
					<div id="_idContainer039">
						{{#each (getChairImages this.wheelchair)}}
						<img class="_idGenObjectAttribute-1" src="{{this}}" alt="" />
						{{/each}}
					</div>
          <p class="image-hint-text">
              If the image rendering above is incomplete, you can view it at <a href="{{BASE_URL}}#!/tinker/{{this._id}}">{{BASE_URL}}#!/tinker/{{this._id}}</a>
          </p>
					<div id="_idContainer043" class="Basic-Text-Frame">
						{{#with (getChairFrame this.wheelchair) as |frame|}}
						<p class="Numbers" style="height: 15px; margin-top: 0px;">{{frame.manufacturer}}</p>
						{{/with}}
						<p class="Numbers" style="height: 15px; margin-top: 0px;">${{getChairPrice this.wheelchair ..}}</p>
						{{#with (getChairFrame this.wheelchair) as |frame|}}

                <p class="Numbers" style="height: 15px; margin-top: 0px;">{{frame.name}}</p>
						{{/with}}
						<p class="Numbers" style="height: 15px; margin-top: 0px;">{{displayDate this.updatedAt}}</p>

              <p class="Numbers" style="height: 15px; margin-top: 0px;">1</p>
              <p class="Numbers" style="height: 15px; margin-top: 0px;">${{this.wheelchair.grantAmount}}</p>

          </div>
				</div>
			</div>
			<!--Paginated Parts Page-->
			{{#each (getPaginatedParts this.wheelchair)}}
			<div class="_idGenObjectLayout-1">
				<div id="_idContainer050">
					<div id="_idContainer035">
						<div id="_idContainer031" class="Basic-Graphics-Frame _idGenObjectStyleOverride-1">
						</div>
						<div id="_idContainer032" class="Basic-Text-Frame">
						</div>
						<div id="_idContainer033" class="Basic-Text-Frame">
							<p class="Header">Powered By </p>
						</div>
						<div id="_idContainer034" class="_idGenObjectStyleOverride-2">
							<img class="_idGenObjectAttribute-1" src="data:image/png;base64," alt="" />
						</div>
					</div>
					<div id="wrapper">
						<header class="">
							{{#if @first}}
							<h2>Parts</h2>
							{{else}}
							<h2>Parts (cont.)</h2>
							{{/if}}
						</header>

						<body>

							<section class="title">
								<div class="parts">PARTS</div>
								<div class="details">DETAILS</div>
								<div class="price">PRICE</div>
							</section>
							{{#each this}}
							<div>
								<section class="optionTable">
									<div class="parts themeBlue">{{this.part.name}}</div>
									<div class="details">{{this.option.name}}<span class="colorInfo">{{getChairOptionColorName ../../wheelchair this.option.optionID}}</span> {{getOptionSize ../../wheelchair this.option.optionID}} inch</div>
									<div class="price">${{getChairPartOptionPrice ../../wheelchair this.option.optionID}}</div>
								</section>
								<section class="comments">
									<p>  {{getChairOptionComment ../../wheelchair this.option.optionID}}</p>
								</section>
							</div>
							{{/each}}
						</body>
					</div>
				</div>
			</div>
			{{/each}}
			<!--Measurements Page-->
			{{#each (getPaginatedMeasures this.wheelchair)}}
			<div class="_idGenObjectLayout-1">
				<div id="_idContainer050">
					<div id="_idContainer035">
						<div id="_idContainer031" class="Basic-Graphics-Frame _idGenObjectStyleOverride-1">
						</div>
						<div id="_idContainer032" class="Basic-Text-Frame">
						</div>
						<div id="_idContainer033" class="Basic-Text-Frame">
							<p class="Header">Powered By </p>
						</div>
						<div id="_idContainer034" class="_idGenObjectStyleOverride-2">
							<img class="_idGenObjectAttribute-1" src="data:image/png;base64," alt="" />
						</div>
					</div>
					<div id="wrapper">
						<header>
							{{#if @first}}
							<h2>Measurements</h2>
							{{else}}
							<h2>Measurements (cont.)</h2>
							{{/if}}
						</header>

						<body>
							<section>
							</section>

							<section class="title">
								<div class="mParts">PARTS</div>
								<div class="mInput">MEASUREMENT</div>
							</section>

							{{#each this}}
							{{#with (getChairMeasureOption ../../wheelchair this.measureID) as |measureInfo|}}
							<div>
								<section class="optionTable">
									<div class="mParts themeBlue" style="float:left;">{{measureInfo.name}}</div>
									<div class="mInput"> {{measureInfo.in}} {{measureInfo.unit}}</div>
								</section>
								<section class="comments">
									<p>  {{measureInfo.comments}}</p>
								</section>
							</div>
							{{/with}}
							{{/each}}
						</body>
					</div>
				</div>
			</div>
			{{/each}}
		{{/each}}
	</body>
</html>
